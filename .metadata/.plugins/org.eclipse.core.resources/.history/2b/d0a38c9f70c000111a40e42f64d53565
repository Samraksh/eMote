#ifndef _PRIORITY_QUEUE_H_
#define _PRIORITY_QUEUE_H_


#define PQSIZE 32

template <class T>
class PriorityQueue
{
	// Body of the queue
	T queue[PQSIZE + 1];

	// Number of elements in the queue
	UINT32 numberOfElements;

};



#endif
