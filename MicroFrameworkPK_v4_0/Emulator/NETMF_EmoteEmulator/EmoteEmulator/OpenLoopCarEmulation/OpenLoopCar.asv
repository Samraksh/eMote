a=[423 613];
c=[367.74 764.83]
r=161.567;

v=50;

%Initial angle from y-axis
alpha = 2 *  pi - atan2(a(2)-c(2),a(1)-c(1))

%Determine angular displacement
theta = v/r;

figure();
hold on;
plot (c(1),c(2),'og');
plot(a(1),a(2),'ob');

for i=1:10
    x(i)=c(1)+r* cos(alpha+theta);
    y(i)=c(1)+r* cos(alpha+theta);
    
    plot(x(i),y(i),'o
end






hold off;

